{
  "name": "stat-monitor",
  "version": "0.0.1",
  "description": "Stat Monitor",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dockerBuild": "docker build -t mitwilsch/stat-monitor:latest .",
    "prodClient": "npx http-server dist -p 80",
    "prodServer": "node src/server/index.js",
    "start": "npx concurrently \"npm run prodClient\" \"npm run prodServer\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.15",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint_d": "^8.1.1",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.13",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.43.0",
